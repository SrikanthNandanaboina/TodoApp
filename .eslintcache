 = (status, type) => {\r\n    setPopUp(status);\r\n    setpopUpType(type);\r\n  };\r\n\r\n  const togglePopUp = async (data) => {\r\n    let response;\r\n    let stateTasks = [...tasks];\r\n    if (data.type === \"Add\") {\r\n      response = await TasksController.addTask({ title: data.taskName }, token);\r\n    } else {\r\n      response = await TasksController.editTask(\r\n        data.id,\r\n        { title: data.taskName, status: popUpType.completed },\r\n        token\r\n      );\r\n    }\r\n    if (!response.err) {\r\n      if (data.type === \"Add\") stateTasks = [...stateTasks, response.data];\r\n      else\r\n        stateTasks[data.index] = {\r\n          ...stateTasks[data.index],\r\n          title: data.taskName,\r\n        };\r\n      setData(stateTasks);\r\n      setAddorEdit(false, null);\r\n    } else {\r\n      alert(\"Something went wrong !!\");\r\n    }\r\n  };\r\n\r\n  const deleteTask = async (index, id) => {\r\n    const { err } = await TasksController.deleteTask(id, token);\r\n    if (!err) {\r\n      const stateTasks = [...tasks];\r\n      stateTasks.splice(index, 1);\r\n      setData(stateTasks);\r\n    } else {\r\n      if (err.status === 400) alert(\"Marked task can't be deleted\");\r\n    }\r\n  };\r\n\r\n  const markReadUnRead = async (data) => {\r\n    console.log(data);\r\n    const status = data.completed === 0 ? 1 : 0;\r\n    const { err } = await TasksController.editTask(\r\n      data.id,\r\n      { title: data.value, status },\r\n      token\r\n    );\r\n    if (!err) {\r\n      const stateTasks = [...tasks];\r\n      stateTasks[data.index] = { ...stateTasks[data.index], completed: status };\r\n      setData(stateTasks);\r\n    }\r\n  };\r\n\r\n  return (\r\n    <>\r\n      {showPopUp && (\r\n        <AddOrEditItem\r\n          popUpType={popUpType}\r\n          setAddorEdit={setAddorEdit}\r\n          togglePopUp={togglePopUp}\r\n        />\r\n      )}\r\n      <NavBar props={props} />\r\n      {tasks.length ? (\r\n        <>\r\n          <Cards latestTasks={latestTasks} tasksInfo={tasksInfo} />\r\n          <AddOrSearchTask\r\n            setAddorEdit={setAddorEdit}\r\n            setSearchText={setSearchText}\r\n          />\r\n          <TasksList\r\n            searchText={searchText}\r\n            markReadUnRead={markReadUnRead}\r\n            setAddorEdit={setAddorEdit}\r\n            tasks={tasks}\r\n            deleteTask={deleteTask}\r\n          />\r\n        </>\r\n      ) : (\r\n        <NoTasks setAddorEdit={setAddorEdit} />\r\n      )}\r\n    </>\r\n  );\r\n};\r\n\r\nexport default withRouter(Dashboard);\r\n","C:\\Users\\srika\\Desktop\\Other\\tdcx\\src\\components\\login\\index.jsx",[],"C:\\Users\\srika\\Desktop\\Other\\tdcx\\src\\utilities\\tasksController.js",[],"C:\\Users\\srika\\Desktop\\Other\\tdcx\\src\\utilities\\loginController.js",[],"C:\\Users\\srika\\Desktop\\Other\\tdcx\\src\\components\\dashboard\\navbar\\index.jsx",[],"C:\\Users\\srika\\Desktop\\Other\\tdcx\\src\\components\\dashboard\\addOrSearchTask\\index.jsx",[],"C:\\Users\\srika\\Desktop\\Other\\tdcx\\src\\components\\dashboard\\cards\\index.jsx",[],"C:\\Users\\srika\\Desktop\\Other\\tdcx\\src\\components\\dashboard\\tasksList\\index.jsx",[],"C:\\Users\\srika\\Desktop\\Other\\tdcx\\src\\components\\dashboard\\addOrEditItem\\index.jsx",[],"C:\\Users\\srika\\Desktop\\Other\\tdcx\\src\\components\\dashboard\\noTasks\\index.jsx",[],"C:\\Users\\srika\\Desktop\\Other\\tdcx\\src\\components\\dashboard\\cards\\styles.jsx",[],"C:\\Users\\srika\\Desktop\\Other\\tdcx\\src\\components\\dashboard\\tasksList\\styles.jsx",[],"C:\\Users\\srika\\Desktop\\Other\\tdcx\\src\\components\\dashboard\\addOrSearchTask\\styles.jsx",[],"C:\\Users\\srika\\Desktop\\Other\\tdcx\\src\\components\\dashboard\\navbar\\styles.jsx",[],"C:\\Users\\srika\\Desktop\\Other\\tdcx\\src\\components\\dashboard\\addOrEditItem\\styles.jsx",[],"C:\\Users\\srika\\Desktop\\Other\\tdcx\\src\\components\\dashboard\\noTasks\\styles.jsx",[],"C:\\Users\\srika\